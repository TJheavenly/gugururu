{
  "name": "ぐぐるる",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Google検索",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["main.js"]
    }],
  "browser_action": {
    "default_title": "ぐぐるる",
    "default_popup": "google.html"
  },
  "commands": {
  "_execute_browser_action": {
    "suggested_key": {
      "default": "Ctrl+Shift+F",
      "mac": "MacCtrl+Shift+F"
      }
    }
  }
}
